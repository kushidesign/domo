;; shadow-cljs configuration
{:deps
 {:aliases [:local-dev]}

 :dev-http
 {8020 "public"}

 :builds
 {:app {:devtools   {:log             false
                     :reload-strategy :full
                     :watch-dir       "public"}
        :target     :browser
        :output-dir "public/js"
        :asset-path "/js"

        :modules    {:main ; becomes public/js/main.js
                     {:init-fn starter.browser/init}}}}}
